meta {
  name: health test
  type: http
  seq: 3
}

get {
  url: {{BASE_URL}}/resources
}

assert {
  res.status: in [200]
}

script:post-response {
    const resources = res.getBody();
    const myResource = resources.find(r => r.name === "listru-test-unique");

    if (myResource) {
        bru.setVar("delId", myResource.id);
        console.log("Found Resource ID: " + myResource.id);
    }
}
